{
    "categories": [
        {
            "title": "âœ¨ Features",
            "labels": [
                "feature",
                "feat"
            ]
        },
        {
            "title": "ğŸ› Bug Fixes",
            "labels": [
                "fix",
                "bug"
            ]
        },
        {
            "title": "ğŸ“¦ Dependencies",
            "labels": [
                "deps",
                "dependency"
            ]
        },
        {
            "title": "ğŸ“ Documentation",
            "labels": [
                "docs",
                "documentation"
            ]
        },
        {
            "title": "âš¡ Performance",
            "labels": [
                "perf",
                "performance"
            ]
        },
        {
            "title": "â™»ï¸ Refactor",
            "labels": [
                "refactor"
            ]
        },
        {
            "title": "ğŸ¨ Style",
            "labels": [
                "style"
            ]
        },
        {
            "title": "ğŸ§ª Tests",
            "labels": [
                "test",
                "tests"
            ]
        },
        {
            "title": "ğŸ”§ Configuration",
            "labels": [
                "config",
                "chore"
            ]
        }
    ],
    "ignore_labels": [
        "ignore",
        "no-changelog"
    ],
    "sort": "ASC",
    "template": "#${{TITLE}}\n\n${{CHANGELOG}}\n\n",
    "pr_template": "- ${{TITLE}} #${{NUMBER}}",
    "commit_template": "- ${{MESSAGE}}",
    "empty_template": "No significant changes in this release.",
    "label_extractor": [
        {
            "pattern": "^(feat|feature)(\\s*\\(.*?\\))?:",
            "target": "feature",
            "on_property": "message"
        },
        {
            "pattern": "^fix(\\s*\\(.*?\\))?:",
            "target": "fix",
            "on_property": "message"
        },
        {
            "pattern": "^(deps|dependency)(\\s*\\(.*?\\))?:",
            "target": "dependency",
            "on_property": "message"
        },
        {
            "pattern": "^docs(\\s*\\(.*?\\))?:",
            "target": "docs",
            "on_property": "message"
        },
        {
            "pattern": "^perf(\\s*\\(.*?\\))?:",
            "target": "perf",
            "on_property": "message"
        },
        {
            "pattern": "^refactor(\\s*\\(.*?\\))?:",
            "target": "refactor",
            "on_property": "message"
        },
        {
            "pattern": "^style(\\s*\\(.*?\\))?:",
            "target": "style",
            "on_property": "message"
        },
        {
            "pattern": "^test(\\s*\\(.*?\\))?:",
            "target": "test",
            "on_property": "message"
        },
        {
            "pattern": "^(chore|config)(\\s*\\(.*?\\))?:",
            "target": "chore",
            "on_property": "message"
        },
        {
            "pattern": ".*\\[skip ci\\].*",
            "target": "ignore",
            "on_property": "message"
        },
        {
            "pattern": "^chore: bump to dev version.*",
            "target": "ignore",
            "on_property": "message"
        },
        {
            "pattern": "^chore: release version.*",
            "target": "ignore",
            "on_property": "message"
        },
        {
            "pattern": "^Merge pull request.*",
            "target": "ignore",
            "on_property": "message"
        },
        {
            "pattern": "^Merge branch.*",
            "target": "ignore",
            "on_property": "message"
        },
        {
            "pattern": "^(Update|CI|Fix|Fixes|Tests) .*",
            "target": "chore"
        }
    ],
    "duplicate_filter": {
        "pattern": "^(v)?\\d+\\.\\d+\\.\\d+.*",
        "on_property": "title"
    },
    "base_branches": [
        "main"
    ]
}