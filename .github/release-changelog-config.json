{
    "categories": [
        {
            "title": "âœ¨ Features",
            "labels": [
                "feature",
                "feat"
            ]
        },
        {
            "title": "ğŸ› Bug Fixes",
            "labels": [
                "fix",
                "bug"
            ]
        },
        {
            "title": "ğŸ“ Documentation",
            "labels": [
                "docs"
            ]
        },
        {
            "title": "ğŸ“¦ Dependencies",
            "labels": [
                "dependency"
            ]
        },
        {
            "title": "ğŸ”§ Maintenance & CI",
            "labels": [
                "chore"
            ]
        },
        {
            "title": "ğŸ§ª Tests",
            "labels": [
                "test"
            ]
        }
    ],
    "ignore_labels": [
        "ignore",
        "no-changelog"
    ],
    "sort": "ASC",
    "template": "# Changelog\n\n#{{CHANGELOG}}\n",
    "pr_template": "- #{{TITLE}} (#{{NUMBER}})",
    "commit_template": "- #{{TITLE}}",
    "empty_template": "No significant changes in this release.",
    {
        "pattern": "^([Ff]eat|[Ff]eature|[Aa]dd)(\\s*\\(.*?\\))?:",
        "target": "âœ¨ Features",
        "on_property": "message"
    },
    {
        "pattern": "^[Nn]ew ([Ff]eat|[Ff]eature).*",
        "target": "âœ¨ Features",
        "on_property": "message"
    },
    {
        "pattern": "^[Ff]ix(\\s*\\(.*?\\))?:",
        "target": "ğŸ› Bug Fixes",
        "on_property": "message"
    },
    {
        "pattern": "^[Ff]ix(ed|es)? .*",
        "target": "ğŸ› Bug Fixes",
        "on_property": "message"
    },
    {
        "pattern": "^[Dd]ocs(\\s*\\(.*?\\))?:",
        "target": "ğŸ“ Documentation",
        "on_property": "message"
    },
    {
        "pattern": "^[Uu]pdate .*\\.md$",
        "target": "ğŸ“ Documentation",
        "on_property": "message"
    },
    {
        "pattern": "^[Dd]ocs improvements.*",
        "target": "ğŸ“ Documentation",
        "on_property": "message"
    },
    {
        "pattern": "^[Dd]eps(\\s*\\(.*?\\))?:",
        "target": "ğŸ“¦ Dependencies",
        "on_property": "message"
    },
    {
        "pattern": "^[Tt]est(\\s*\\(.*?\\))?:",
        "target": "ğŸ§ª Tests",
        "on_property": "message"
    },
    {
        "pattern": "^([Cc]hore|[Cc]onfig|[Cc][Ii]|[Ww]orkflow|[Rr]uff)(\\s*\\(.*?\\))?:",
        "target": "ğŸ”§ Maintenance & CI",
        "on_property": "message"
    },
    {
        "pattern": "^[Uu]pdate release\\.yml.*",
        "target": "ğŸ”§ Maintenance & CI",
        "on_property": "message"
    },
    {
        "pattern": ".*\\[skip ci\\].*",
        "target": "ignore",
        "on_property": "message"
    },
    {
        "pattern": "^([Cc][Ii] fix|[Cc]hore: auto-fix|[Cc]hore: bump).*",
        "target": "ignore",
        "on_property": "message"
    },
    {
        "pattern": "^[Nn]ew test file.*",
        "target": "ğŸ§ª Tests",
        "on_property": "message"
    },
    {
        "pattern": "^[Cc]hore: release version.*",
        "target": "ignore",
        "on_property": "message"
    },
    {
        "pattern": "^[Rr]elease workflow fixes.*",
        "target": "ğŸ”§ Maintenance & CI",
        "on_property": "message"
    },
    {
        "pattern": "^[Mm]erge (pull request|branch).*",
        "target": "ignore",
        "on_property": "message"
    },
    {
        "pattern": "^([Uu]pdate|[Cc][Ii]|[Ii]mproved|[Ii]nstructions|[Nn]ew|[Aa]llow|[Nn]ew option) .*",
        "target": "ğŸ”§ Maintenance & CI",
        "on_property": "message"
    }
    "duplicate_filter": {
        "pattern": "^(v)?\\d+\\.\\d+\\.\\d+.*",
        "on_property": "title"
    },
    "base_branches": [
        "main"
    ]
}